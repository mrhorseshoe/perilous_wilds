<h1>Discovery</h1>
<ul>
  <% @discovery.elements.each do |element| %>
  <li><%= element.to_s %></li>
    <% element.details.each do |name, value| %>
    <dl>
      <dt><%= name %></dt>
      <dd><%= value %></dd>
    </dl>
    <% end %>
  <% end %>
</ul>

<a href="/discovery">reload</a><br>
<a href="/welcome">back</a>
